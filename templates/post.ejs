<% include views/include/bootstrap %>
<% include views/include/fonts %>
<% include views/include/css-common %>
<% include views/include/code-prettify %>

<html>
<head>
  <% include views/common/head-meta-default %>
  
  <title><%= title %></title>
  
  <%- this.helpers.css_load_html %>
  <% if(typeof custom_css !== 'undefined' && custom_css) { %>
    <%- custom_css %>
  <% } %>
</head>
<body>
  <div class="container">
    <h1><%= title %><br><small><%= date %></small></h1>
    
    <div class="row">
      <div id="contents" class="col-lg-12 col-md-12 col-sm-12">
        <%- contents %>
      </div>
    </div>
    
  </div>
  <% include views/footer %>
  <%- this.helpers.js_load_html %>
</body>
</html>
